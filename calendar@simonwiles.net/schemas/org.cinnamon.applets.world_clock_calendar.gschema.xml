<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="cinnamon-applets-world_clock_calendar">
    <schema path="/org/cinnamon/applets/world_clock_calendar/" id="org.cinnamon.applets.world_clock_calendar">
        <key type="as" name="worldclocks">
            <default>['London|Europe/London']</default>
            <summary>List of time-zones to display</summary>
            <description>A list of time-zones to display in the calendar window. Each entry is in the format "display name|time-zone".</description>
        </key>
        <key name="time-format" type="s">
            <default>'%_I:%M%P (%a)'</default>
            <summary>Time format to use for the world-clocks</summary>
            <description/>
        </key>
    </schema>
</schemalist>
